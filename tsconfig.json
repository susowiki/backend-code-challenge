{
   "compilerOptions": {
      "target": "es2017",
      "module": "commonjs",
      "outDir": "./dist",
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "sourceMap": true,
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "declaration": true,
      "removeComments": true,
      "allowSyntheticDefaultImports": true,
      "baseUrl": "./",
      "incremental": true
   }
}